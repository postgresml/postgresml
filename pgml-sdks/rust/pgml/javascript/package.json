    "url": "https://postgresml.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/postgresml/postgresml"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "cargo-cp-artifact": "^0.1"
  }
}
