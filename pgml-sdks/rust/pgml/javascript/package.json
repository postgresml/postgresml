{
  "name": "pgml",
  "version": "0.1.2",
  "description": "Open Source Alternative for Building End-to-End Vector Search Applications without OpenAI & Pinecone",
  "main": "index.js",
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "build-named": "cargo-cp-artifact -nc $NEON_OUT_NAME -- cargo build --message-format=json-render-diagnostics --release"
  },
  "author": "PostgresML",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "cargo-cp-artifact": "^0.1"
  }
}
